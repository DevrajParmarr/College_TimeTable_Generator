{
  "name": "exam-room-allocation",
  "version": "1.0.0",
  "description": "Room allocation engine for exams with web interface",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "cd frontend && npm run build",
    "install-client": "cd frontend && npm install",
    "heroku-postbuild": "npm run install-client && npm run build",
    "client": "cd frontend && npm start",
    "dev-full": "concurrently \"npm run dev\" \"npm run client\""
  },
  "keywords": [
    "exam",
    "allocation",
    "room",
    "teacher",
    "university"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "axios": "^1.12.2",
    "concurrently": "^9.2.1",
    "console-table-printer": "^2.11.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "mongoose": "^8.19.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
